@model DevelopmentMetrics.Website.Models.BuildStabilityChart

@{
    var myChart = new Chart(width: 1000, height: 600)
        .AddTitle(Model.ChartTitle)
        .AddSeries(
            name: "BuildStability",
            chartType: "Line",
            xValue: Model.SeriesX,
            yValues: Model.SeriesY)
        .AddSeries(
            name: "Build2",
            chartType: "Line",
            xValue: Model.SeriesX,
            yValues: new[] { "30", "50", "75" })
        .Write();
}